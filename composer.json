{
    "name": "smartspending/app",
    "description": "Smart Spending - Personal Finance Management",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "HUYHOANG",
            "email": "huyhoangpro187@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "phpoffice/phpspreadsheet": "^1.29",
        "vlucas/phpdotenv": "^5.5",
        "zircote/swagger-php": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Core\\": "app/Core/",
            "App\\Http\\Controllers\\": "app/Http/Controllers/",
            "App\\Http\\Controllers\\User\\": "app/Http/Controllers/User/",
            "App\\Http\\Controllers\\Admin\\": "app/Http/Controllers/Admin/",
            "App\\Http\\Controllers\\Auth\\": "app/Http/Controllers/Auth/",
            "App\\Models\\": "app/Models/",
            "App\\Services\\": "app/Services/",
            "App\\Middleware\\": "app/Middleware/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
